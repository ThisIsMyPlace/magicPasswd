<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <CheckBoxPreference
        android:key="pref_enabled"
        android:title="@string/pref_enabled_title"
        android:summary="@string/pref_enabled_summ"
        android:defaultValue="false" />
    <ListPreference 
        android:key="pref_type"
        android:title="@string/pref_type_title"
        android:entries="@array/pref_type_entries"
        android:entryValues="@array/pref_type_entries_values"
        android:dialogTitle="@string/pref_type_title"
        android:defaultValue="0" />
</PreferenceScreen>
